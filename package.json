{
  "name": "@clubmed/root",
  "version": "1.10.6",
  "description": "",
  "main": "index.js",
  "private": true,
  "repository": {
    "url": "https://github.com/ClubMediterranee/swagger.git"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "cross-env": "5.2.0",
    "highlight.js": "10.0.3",
    "lerna": "3.15.0",
    "lodash": "4.17.15",
    "lucene-filter": "0.1.3",
    "lucene-query-parser": "1.2.0",
    "moment": "2.24.0",
    "normalize.css": "4.1.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-app-polyfill": "1.0.1",
    "react-copy-to-clipboard": "5.0.2",
    "react-dom": "16.13.1",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "swagger-ui-react": "3.25.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "eslint": "6.1.0",
    "eslint-config-react-app": "5.0.2",
    "eslint-config-standard": "12.0.0",
    "eslint-import-resolver-lerna": "1.1.0",
    "eslint-loader": "3.0.0",
    "eslint-plugin-cypress": "2.6.0",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-mocha": "5.3.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.6.1",
    "eslint-plugin-should-promised": "2.0.0",
    "eslint-plugin-standard": "4.0.0",
    "fs-extra": "7.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.2.0"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "build": "yarn build:features && yarn build:swagger",
    "build:swagger": "cross-env PUBLIC_URL=${BASE_URL}/swagger-ui lerna run build --stream --scope @clubmed/swagger-ui",
    "build:features": "lerna run build --stream --scope @clubmed/features-and-fix",
    "start:swagger": "lerna run start --stream --no-prefix --scope @clubmed/swagger-ui",
    "test": "cross-env CI=true lerna run test --concurrency 2 -- --passWithNoTests",
    "lint:fix": "lerna run lint:fix --parallel",
    "start:prod": "serve",
    "release": "cmrelease",
    "release:dryrun": "cmrelease --dry-run"
  },
  "author": "Lenzotti Romain",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint:fix",
      "git add"
    ]
  }
}
